//dropship_interior---------------------------------------------------------------------------------------------------------

models/vehicles/dropship_interior/interior
{
	noselfShadow
	unsmoothedtangents

	renderBump  -size 1024 1024 -aa 2 -trace 0.05 -MergeHigh models/vehicles/dropship_interior/interior_local.tga work/models/vehicles/dropship_interior/dropship_interior_hi_render.lwo
	materialImage	models/vehicles/dropship_interior/interior_hit
	
	bumpmap		addnormals (models/vehicles/dropship_interior/interior_local.tga, heightmap (models/vehicles/dropship_interior/interior_h.tga, 2))
	diffusemap		models/vehicles/dropship_interior/interior_d.tga
	specularmap		models/vehicles/dropship_interior/interior_s.tga

// lights
	
	{
		if		(Parm4 == 0)						// normal light
		blend add
		map models/vehicles/dropship_interior/interior_glow.tga
		red		parm0
		green	parm1
		blue	parm2		
		zeroClamp
	}
	{
		if		(Parm4 > 0)						// dim and then get back normal
		blend add
		map models/vehicles/dropship_interior/interior_glow.tga
		red		airdefense_global_dim_a[(time-Parm4)*0.4]*parm0
		green	airdefense_global_dim_a[(time-Parm4)*0.4]*parm1
		blue		airdefense_global_dim_a[(time-Parm4)*0.4]*parm2
		zeroClamp
	}
	
//runner lights

	{
		if (Parm5 == 0)
		blend add
		map models/vehicles/dropship_interior/interior_glow2.tga
	}
	{
		if (Parm5 == 1)
		blend add
		map models/vehicles/dropship_interior/interior_glow2.tga
		rgb flicker [time * 3]
	}

//color change

	{
		if ( Parm7 == 0 )
		blend 	add
		map models/vehicles/dropship_interior/interior_glow3.tga
		red		1
		green	0.1
		blue	0.1

	}
	{
		if ( Parm7 == 1 )
		blend 	add
		map models/vehicles/dropship_interior/interior_glow3.tga
		red		0.6
		green	1
		blue	0.23
	}
	//red light
	{
		blend add
		map models/vehicles/dropship_interior/interior_glow4.tga
	}
	
	{	
		maskcolor				
	    map	makealpha(models/vehicles/dropship_interior/interior_mask.tga)
	}

	{
	    blend GL_DST_ALPHA, gl_one
	    maskalpha
       	cubeMap		gfx/env/marhelmet
        texgen		reflect
		red   Parm0
		green Parm1
		blue  Parm2
	}
	
}		

