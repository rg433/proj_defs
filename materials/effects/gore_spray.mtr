	DECAL_MACRO
	noShadows
	noimpact
	translucent
//	blood

	{
      	blend		gl_zero, gl_one_minus_src_color
		Map gfx/effects/gore_spray/blooddrip1.tga
		vertexColor
		rgba		decalFade[ DecalLife ]
