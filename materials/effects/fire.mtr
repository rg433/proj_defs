gfx/effects/fire/firetest
{
	noSelfShadow
	noshadows
	twosided
	translucent
	nonsolid
	noimpact

	qer_editorimage gfx/effects/fire/flame.tga
	{
		if ( ( time * 6 ) % 6 == 0 )
		blend	add
		map		gfx/effects/fire/fire_a.tga
		vertexcolor
	}	
	{
		if ( ( time * 6 ) % 6 == 1 )
		blend	add
		map		gfx/effects/fire/fire_b.tga
		vertexcolor
	}	
	{
		if ( ( time * 6 ) % 6 == 2 )
		blend	add
		map		gfx/effects/fire/fire_c.tga
		vertexcolor
	}	
	{
		if ( ( time * 6 ) % 6 == 3 )
		blend	add
		map		gfx/effects/fire/fire_d.tga
		vertexcolor
	}	
	{
		if ( ( time * 6 ) % 6 == 4 )
		blend	add
		map		gfx/effects/fire/fire_e.tga
		vertexcolor	
	}	
	{
		if ( ( time * 6 ) % 6 == 5 )
		blend	add
		map		gfx/effects/fire/fire_f.tga
		vertexcolor
	}
}



gfx/effects/fire/burst
{
	qer_editorimage gfx/effects/fire/burst_strip.tga
	noSelfShadow
	translucent
	noShadows
	twosided
	deform sprite
	{
		blend add
		map gfx/effects/fire/burst_strip.tga
		scale 1 / 32 , 1
		scroll table32[ time * .8 ] , 0
		clamp
	}
}


