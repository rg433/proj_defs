gfx/effects/fire/fire_napalm
{
	qer_editorimage gfx/effects/fire/fire_napalm.tga
	translucent
	twoSided
	noShadows
	{
		blend add
		map gfx/effects/fire/fire_napalm.tga
		scroll -1.5 * time ,0
		vertexColor
	}
		{
		blend add
		map gfx/effects/fire/fire_napalm.tga
		scroll time * -.5 ,0
		scale 2, 2
		vertexColor
	}

}

