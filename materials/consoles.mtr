guide textures/consoles/conmonitorxl1b generic_materialimageshader ("consoles/conmonitorxl1b")

textures/consoles/conmonitorxl1b_glow
{
	qer_editorimage textures/consoles/conmonitorxl1b_d
	materialImage textures/consoles/conmonitorxl1b_hit

	bumpmap addnormals ( textures/consoles/conmonitorxl1b_local, heightmap ( textures/consoles/conmonitorxl1b_h, 1 ) )
	diffusemap	textures/consoles/conmonitorxl1b_d
	specularmap	textures/consoles/conmonitorxl1b_s
	{
		blend	add
		map		textures/consoles/conmonitorxl1a_add

		red		parm0  
		green	parm1  
		blue	parm2  
	}
}

guide textures/consoles/conmonitorxl1c generic_materialimageshader ("consoles/conmonitorxl1c")
guide textures/consoles/conmonitorxl1d generic_Localvariant_mi ("consoles/conmonitorxl1c", "consoles/conmonitorxl1d")

