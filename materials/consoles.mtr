guide textures/consoles/console1b generic_typeshader ("consoles/console1b", "electronics")
guide textures/consoles/console1c generic_typeshader ("consoles/console1c", "electronics")
guide textures/consoles/console1d generic_Localvariant_typeshader ("consoles/console1c", "consoles/console1d", "electronics")

textures/consoles/console1e
{
	qer_editorimage textures/consoles/console1e_d
	materialType	electronics

	bumpmap addnormals ( textures/consoles/console1c_local, heightmap ( textures/consoles/console1e_h, 1 ) )
	diffusemap	textures/consoles/console1e_d
	specularmap	textures/consoles/console1e_s
	{
	blend 	add
	map	textures/consoles/console1e_add.tga
	rgb	5
	}


	{
	blend 	add
	map	textures/consoles/console1e_add2.tga
	rgb 	(3 * sintable [.2 - time ]) +.2
	}
}

