textures/stroyent/belt
{
	qer_editorimage textures/stroyent/belt_local.tga



	{
		blend	bumpmap
		map		addnormals( textures/stroyent/belt_local.tga, heightmap ( textures/stroyent/belt_h.tga, 1 ) )
		scroll	0, time * 3
	}
	
	
	
	{
		blend	diffusemap
		map		textures/stroyent/belt_d.tga
		scroll	0, time * 3
	}
	{
		blend	specularmap
		map		textures/stroyent/belt_s.tga
		scroll	0, time * 3
	}




}



textures/stroyent/thinwavy
{
	noSelfShadow
	noShadows
	translucent
	
	qer_EditorImage textures/stroyent/thinwavy_d.tga

	{       
      	blend		diffuseMap
		map			textures/stroyent/thinwavy_d.tga
	}
	
	{
		
		blend blend
		map	textures/stroyent/foam_2_d.tga 	//for light decals
		translate	.025 * ( sinTable[time * .02 ]),.025 * (sintable[time * .02])
		scale		1 - ( halfsinTable[(time * .03) + 5 ] * .1 ),1 - ( halfsinTable[(time * .05) ] * .1 )
		rotate		.03 * (sinTable [time * .01 ])

	}
}

