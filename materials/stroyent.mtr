textures/stroyent/stroy_conveyor
{
	qer_EditorImage textures/stroyent/stroy_conveyor_d.tga
	noOverlays

	{
	blend bumpMap	
	map addnormals ( textures/stroyent/stroy_conveyor_local.tga, heightmap ( textures/stroyent/stroy_conveyor_h.tga, 2 ) )
	scroll 0, time * .66
	}
	
	{
	blend diffuseMap
	map	textures/stroyent/stroy_conveyor_d.tga
	scroll 0, time * .66
	}
	
	{
	blend specularMap
	map	textures/stroyent/stroy_conveyor_s.tga
	scroll 0, time * .66
	}
}

