models/mapobjects/strogg/medlabs/lab_arm/tube_fluid
{
	noShadows
	translucent
	nonSolid
//	twoSided	
	deform	turbulent	sintable .0295 time * .2 1
	
	
	{
		if ( glslPrograms )== 1
		blend blend
		
		glslProgram	glsl/Displacement.glsl
		shaderParm		scrollX				0
		shaderParm		scrollY				time * .65
		shaderParm		sizeX				sintable[time * .0552]
		shaderParm		sizeY				.25
		shaderParm		texCoordSize		.25
		shaderTexture	Image				textures/medlabs/fluid_green_bubble1.tga
		shaderTexture	DisplacementMap		gfx/test/pnh3.tga
	}
	{ 
		If ( glslPrograms )== 1		
		blend add
		map textures/medlabs/fluid_green_bubble2.tga
		scroll	0, time * .15
	}
	
	{
		If ( glslPrograms )== 1		
		blend add
		map textures/medlabs/fluid_green_bubble3.tga
		scroll 0, time * .015
		scale 2, 2
	}
//if no glsl

	{
		if ( glslPrograms )== 0
		blend add	
		map textures/medlabs/fluid_green_bubble5.tga
		scroll 0, time * .025
		scale 3, 3
	}

	{ 
		If ( glslPrograms )== 0		
		blend add
		map textures/medlabs/fluid_green_bubble2.tga
		scroll	0, time * .15
		scale 3, 3
	}
	
	{
		If ( glslPrograms )== 0		
		blend add
		map textures/medlabs/fluid_green_bubble3.tga
		scroll 0, time * .015
		scale 3, 3
	}
}
