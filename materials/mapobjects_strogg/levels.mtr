models/mapobjects/strogg/medlabs/lab_arm/tube_glass
{
//	collision
//	noSelfShadow
	unsmoothedtangents
//	noShadows
	twoSided	
	translucent
	forceOverlays
//	sort decal

	qer_editorimage textures/glass/medlabs
	materialType glass
/*	{
                Program           heatHaze.vfp
                vertexParm              0       time * 0 , time * 0 // texture scrolling
                vertexParm              1       .4         // magnitude of the distortion
                fragmentProgram			heatHaze.vfp
                fragmentMap             0       _currentRender
                fragmentMap             1       gfx/env/vp1   // the normal map for distortion			
             
    }
*/	
	{	
		maskcolor				
	    map	makealpha(textures/glass/medlabs2.tga)
	}

	{
	    blend GL_DST_ALPHA, gl_one
	    maskalpha
		cubeMap		gfx/env/medic
        texgen		reflect
		red   Parm0
		green Parm1
		blue  Parm2
	}
	{
		blend specularMap
		map models/mapobjects/strogg/medlabs/lab_arm/tube_glass_s.tga
	}
}




models/mapobjects/strogg/medlabs/lab_arm/tube_glass2
{
	unSmoothedTangents
	twoSided	
	translucent
	forceOverlays

	qer_editorimage textures/glass/medlabs
	materialType glass
	{	
		if (Parm4) == 0
		maskcolor				
	    map	makealpha(textures/glass/medlabs2.tga)
	}

	{
	    if (Parm4) == 0
	    blend GL_DST_ALPHA, gl_one
	    maskalpha
		cubeMap		gfx/env/medic
        texgen		reflect
		red   Parm0
		green Parm1
		blue  Parm2
	}
	{
		
		if (Parm4) == 0
		blend specularMap
		map models/mapobjects/strogg/medlabs/lab_arm/tube_glass_s.tga
	}
	{
		if (Parm4) == 1
		blend add
		map _black
	}
}

