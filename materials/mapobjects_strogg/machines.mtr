//--------------------------------------------------------------------------
models/mapobjects/strogg/machines/teleport_pad/dataport_fade
{	//fading version of the dataport

      unsmoothedTangents
      noselfShadow
	sort		close

	bumpmap		addnormals(models/mapobjects/strogg/machines/teleport_pad/dataport_local.tga,heightmap(models/mapobjects/strogg/machines/teleport_pad/dataport_h.tga,1))
	diffusemap		models/mapobjects/strogg/machines/teleport_pad/dataport_d.tga
	specularmap		models/mapobjects/strogg/machines/teleport_pad/dataport_s.tga


// abnormal stuff...

	{		//Circuitry glow effect, stays after fade
	if		(global4 > 0)
	blend		add
	map		models/mapobjects/strogg/machines/teleport_pad/dataport_glow.tga
	IgnoreAlphaTest
	red		global4 + Flicker[time] - 2.6
	green		global4 + Flicker[time] - 2.6
	blue		global4 + Flicker[time] - 2.6
	}

	{		//Chipping texture 
	blend		GL_ZERO, GL_ONE
	map 		textures/Stream/Stream_Patchwork_Alpha.tga
	uncompressed
	AlphaTest	-.5 + global4
	}

	{		//Glow effect of the patchwork lines, before chip
	if		(global4> 0 && global4< .6)
	blend		add
	map		textures/Stream/Stream_Patchwork_lines.tga
	IgnoreAlphaTest
	red		global4 * Flicker[time] * 2
	green		global4 * Flicker[time] * 2
	blue		global4 * Flicker[time] * 2
	}

	{		//Glow effect of the patchwork lines, after chip
	if		(global4>.6 && global4<= 3)
	blend		add
	map		textures/Stream/Stream_Patchwork_lines.tga
	IgnoreAlphaTest
	red		.2 + Flicker[time] - WallFade[global4- 2]
	green		.2 + Flicker[time] - WallFade[global4- 2]
	blue		.2 + Flicker[time] - WallFade[global4- 2]
	}

	{		//Overall flash
	if		(global4>= .3 && global4<=.5)
	blend		add
	IgnoreAlphaTest
	Map		_white
	red		.2 * Arch[5 * (global4- .3)]
	green		.6 * Arch[5 * (global4- .3)]
	blue		.4 * Arch[5 * (global4- .3)]
	}		

}

