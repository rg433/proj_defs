models/mapobjects/strogg/misc_equipment/pipes01
{
      	noselfShadow
	unSmoothedTangents

	renderBump  -size  512  512 -aa 2 -MergeHigh -trace .05  models/mapobjects/strogg/misc_equipment/pipes01_local.tga work/models/mapobjects/strogg/misc_equipment/pipes01_high.lwo
	materialType hollowmetal
	bumpmap		addnormals (models/mapobjects/strogg/misc_equipment/pipes01_local.tga, heightmap (models/mapobjects/strogg/misc_equipment/pipes01_h.tga, 2))
	diffusemap		models/mapobjects/strogg/misc_equipment/pipes01_d.tga
	specularmap		models/mapobjects/strogg/misc_equipment/pipes01_s.tga

}


