models/mapobjects/strogg/outdoor/fence/fence_node_fence
{
	qer_editorimage models/mapobjects/strogg/outdoor/fence/ForceFence_beams2.tga
//	translucent
	twoSided
	noShadows
	nonSolid
	monsterClip
	playerClip
	
	// RAVEN BEGIN
	//
	// Adjusted the beams to listen to shader parm0, parm1 and parm2 so we
	// could fade them out with gui effects
	
	{	//Beam layer (single)
		blend		add
		map models/mapobjects/strogg/outdoor/fence/ForceFence_beams2.tga
		//rgb			(1 + (sintable[time * 2] * .25)) 
		red			(1 + (sintable[time * 2] * .25)) * parm0
		green		(1 + (sintable[time * 2] * .25)) * parm1
		blue		(1 + (sintable[time * 2] * .25)) * parm2

		colored
	}

	{	//Static layer
		blend		add
		map			models/mapobjects/strogg/outdoor/fence/ForceFence_static2.tga
		scale		6,2
		
		red			(.6 + (sintable[time * .2] * .2)) * parm0 
		green		(.1 + (sintable[time * .2] * .15)) * parm1 
		blue		(.1 + (sintable[time * .2] * .15)) * parm2 
		
		/*
		red			(.6 + (sintable[time * .2] * .2)) 
		green		(.1 + (sintable[time * .2] * .15)) 
		blue		(.1 + (sintable[time * .2] * .15)) 
		*/
		translate	LaserFenceStatic[(time * 5) + 3], LaserFenceStatic[time * 2]
	}

	{	//Travelling wave
//		if			((LaserFenceWave[time / 10]) <= 1)
//		zeroClamp
		blend		add
		scale		.5, 1
		map			models/mapobjects/strogg/outdoor/fence/ForceFence_wave1.tga
//		translate	(LaserFenceWave[time / 10]) * 4, 0
		translate	time * -2, 0
		
		//rgb			.4 + (sintable[time * 2] * .2) * parm0 
		
		red			.4 + (sintable[time * 2] * .2) * parm0
		blue		.4 + (sintable[time * 2] * .2) * parm1
		green		.4 + (sintable[time * 2] * .2) * parm2
		colored
	}
//
// RAVEN END
//

/*
	{	//Travelling pulse 1
		if			(LaserFenceOn[time / 15] == 1)
		blend		add
		scale		4,1
		map			models/mapobjects/strogg/outdoor/fence/ForceFence_pulse1.tga
		translate	(time - .5),LaserFenceBarJump[time / 10]
		rgb			1 + (sintable[time * 2] * .25) * parm0 
		colored
	}

	{	//Travelling pulse 2
		if			(LaserFenceOn[(time / 15)] != 1)
		blend		add
		scale		4,1
		map			models/mapobjects/strogg/outdoor/fence/ForceFence_pulse1.tga
		translate	((time - .5) * -1),LaserFenceBarJump[(time / 10) -.5 ]
		rgb			1 + (sintable[(time * 2) + .5] * .25) * parm0 
		colored
	}
*/

}

models/mapobjects/strogg/outdoor/fence/fence_node_fence_blue
{
	qer_editorimage models/mapobjects/strogg/outdoor/fence/ForceFence_beams2_blue.tga
//	translucent
	twoSided
	noShadows
	nonSolid
	monsterClip
	playerClip
	
	// RAVEN BEGIN
	//
	// Adjusted the beams to listen to shader parm0, parm1 and parm2 so we
	// could fade them out with gui effects
	
	{	//Beam layer (single)
		blend		add
		map models/mapobjects/strogg/outdoor/fence/ForceFence_beams2_blue.tga
		//rgb			(1 + (sintable[time * 2] * .25)) 
		red			(1 + (sintable[time * 2] * .25)) * parm0
		green		(1 + (sintable[time * 2] * .25)) * parm1
		blue		(.6 + (sintable[time * 2] * .25)) * parm2

		colored
	}

	{	//Static layer
		blend		add
		map			models/mapobjects/strogg/outdoor/fence/ForceFence_static2.tga
		scale		6,2
		
		red			(.1 + (sintable[time * .2] * .2)) * parm0 
		green		(.1 + (sintable[time * .2] * .15)) * parm1 
		blue		(.1 + (sintable[time * .2] * .15)) * parm2 
		
		translate	LaserFenceStatic[(time * 5) + 3], LaserFenceStatic[time * 2]
	}

	{	//Travelling wave
//		if			((LaserFenceWave[time / 10]) <= 1)
//		zeroClamp
		blend		add
		scale		.5, 1
		map			models/mapobjects/strogg/outdoor/fence/ForceFence_wave1_blue.tga
//		translate	(LaserFenceWave[time / 10]) * 4, 0
		translate	time * -2, 0
		
		//rgb			.4 + (sintable[time * 2] * .2) * parm0 
		
		red			.4 + (sintable[time * 2] * .2) * parm0
		blue		.4 + (sintable[time * 2] * .2) * parm1
		green		.4 + (sintable[time * 2] * .2) * parm2
		colored
	}

}



