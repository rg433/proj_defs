//--------------------------------------------------------------------------


models/mapobjects/strogg/doors/sawtooth/doors_lo_fade
{	//fading version of the door_sawtooth
      noselfShadow

	bumpMap		addnormals(models/mapobjects/strogg/doors/sawtooth/doors_lo_local.tga,heightmap(models/mapobjects/strogg/doors/sawtooth/doors_lo_h.tga,1))
	diffusemap		models/mapobjects/strogg/doors/sawtooth/doors_lo_d.tga
	specularmap		models/mapobjects/strogg/doors/sawtooth/doors_lo_s.tga

// abnormal stuff...

	{		//Chipping texture 
	blend		GL_ZERO, GL_ONE
	map 		textures/Stream/Stream_Patchwork_Alpha.tga
	uncompressed
	AlphaTest	-.5 + global4
	}

	{		//Glow effect of the patchwork lines, before chip
	if		(global4> 0 && global4< .6)
	blend		add
	map		textures/Stream/Stream_Patchwork_lines.tga
	IgnoreAlphaTest
	red		global4* Flicker[time] * 2
	green		global4* Flicker[time] * 2
	blue		global4* Flicker[time] * 2
	}

	{		//Glow effect of the patchwork lines, after chip
	if		(global4>.6 && global4<= 3)
	blend		add
	map		textures/Stream/Stream_Patchwork_lines.tga
	IgnoreAlphaTest
	red		.2 + Flicker[time] - WallFade[global4- 2]
	green		.2 + Flicker[time] - WallFade[global4- 2]
	blue		.2 + Flicker[time] - WallFade[global4- 2]
	}

	{		//Overall flash
	if		(global4>= .3 && global4<=.5)
	blend		add
	IgnoreAlphaTest
	Map		_white
	red		.2 * Arch[5 * (global4- .3)]
	green		.6 * Arch[5 * (global4- .3)]
	blue		.4 * Arch[5 * (global4- .3)]
	}		
}

//-------------------------------------------------------------------

models/mapobjects/strogg/doors/sawtooth/StreamDoorframe
{	//Sawtooth doorframe in the stream

	{
	if (parm4 > 0)
	blend		add
	map		models/mapobjects/strogg/doors/sawtooth/Stream_doorframe_edges.tga
	red		(WallFade[(time - parm4) / 10] / 4) - (.5 + sintable[time] *2)
	green		(WallFade[(time - parm4) / 10] / 2)
	blue		(WallFade[(time - parm4) / 10] / 2)
	}

	{		//Glow effect of the patchwork lines
	if (parm4 > 0)
	blend		add
	map		textures/stream/Stream_Patchwork_lines.tga
	scale		2,2
	red		-1 + Flicker[time] + WallFade[(time - parm4) / 10]
	green		-1 + Flicker[time] + WallFade[(time - parm4) / 10]
	blue		-1 + Flicker[time] + WallFade[(time - parm4) / 10]
	}
}


