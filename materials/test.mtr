		scale		6,2
		red			.9 + (sintable[time * .2] * .2)
		green		.2 + (sintable[time * .2] * .25)
		blue		.2 + (sintable[time * .2] * .25)
		translate	LaserFenceStatic[(time * 5) + 3], LaserFenceStatic[time * 2]
	}

	{	//Travelling pulse 1
		if			(LaserFenceOn[time / 15] == 1)
		blend		add
		map			textures/test/ForceFence_pulse1.tga
		translate	(time - .5),LaserFenceBarJump[time / 10]
		rgb			1 + (sintable[time * 2] * .25)
	}

	{	//Travelling pulse 2
		if			(LaserFenceOn[(time / 15)] != 1)
		blend		add
		map			textures/test/ForceFence_pulse1.tga
		translate	((time - .5) * -1),LaserFenceBarJump[(time / 10) -.5 ]
		rgb			1 + (sintable[(time * 2) + .5] * .25)
	}

	{	//Test fill
		if			(0)
		blend		add
		map 		_white
		red			.25 + (sintable[time * .2] * .1)
		green		.05 + (sintable[time * .2] * .05)
		blue		.05 + (sintable[time * .2] * .05)
	}

}


