textures/waste/fuzz_shield
{
	noSelfShadow
	noShadows
	twosided
	materialType electronics
		translucent

	qer_EditorImage models/mapobjects/strogg/waste/fuzz_shield.tga
	deform	turbulent	sintable .02 time*.01 .5
	{
		if ( glslPrograms )== 1
		blend blend
		alpha			.4
		
		glslProgram		glsl/DisplacementCube.glsl
		shaderParm		scrollX				time * 0.25
		shaderParm		scrollY				time * .5
		shaderParm		sizeX				3 
		shaderParm		sizeY				3 
		shaderParm		texCoordSize		.2
		shaderTexture	DisplacementMap		gfx/effects/depth/noise2.tga
		shaderTexture	Image				models/mapobjects/strogg/waste/fuzz_shield.tga
		shaderParm		EyeVector			viewOrigin
		shaderTexture	CubeImage			cubemap gfx/env/gen3
	}

////if no GLSL

	{
		if ( glslPrograms )== 0
		blend blend
		map models/mapobjects/strogg/waste/fuzz_shield.tga
	}
	{
		if ( glslPrograms )== 0		
		blend GL_ONE, gl_one
       	cubeMap		gfx/env/recomp2
        texgen		reflect
		red Parm0
		green Parm1
		blue Parm2
	}
}

