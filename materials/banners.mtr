//----------------------------------
//  strogg wall banners
//----------------------------------

////////// sideways //////////////


textures/banners/side1
{
	qer_editorimage	textures/banners/side1.tga
	noShadows
	nonSolid
	twoSided
	{
		blend diffuseMap
		map textures/banners/side1_d.tga
		alphaTest 0.5
	}	
	{
		blend 	bumpMap
		map		heightmap (textures/banners/1_h.tga, 1)
	}
	{
		blend 	add
		map	textures/banners/side1_d.tga
		rgb .4
	}
	
	specularMap		textures/banners/side1_s.tga
}

textures/banners/side1_ripped
{
	qer_editorimage	textures/banners/side1_ripped.tga
	noShadows
	nonSolid
	twoSided
	{
		blend diffuseMap
		map textures/banners/side1_ripped_d.tga
		alphaTest 0.5
	}	
	{
		blend 	bumpMap
		map		heightmap (textures/banners/1_h.tga, 1)
	}
	{
		blend 	add
		map	textures/banners/side1_ripped_d.tga
		rgb .4
	}
	
	specularMap		textures/banners/side1_s.tga
}

