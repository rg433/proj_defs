////////// tall //////////////

textures/banners/1_tall_shadow
{
	qer_editorimage	textures/banners/1_tall_shd.tga
	noShadows
	nonSolid
	{
       	blend GL_ZERO, GL_ONE_MINUS_SRC_COLOR								//inverse multiplicative blend
		Map textures/banners/1_tall_shd.tga
		red Parm4
		green Parm4
		blue Parm4
	}
}

