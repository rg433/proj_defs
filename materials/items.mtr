models/cinematics/rocket_cin/rocket
{
	noselfShadow
	unsmoothedTangents

	renderbump  -size 512 512 -aa 2 -trace .07 -mergehigh models/cinematics/rocket_cin/rocket_local.tga work/models/cinematics/rocket_cin/rocket_hi.lwo

	diffusemap	models/cinematics/rocket_cin/rocket_d.tga
	bumpmap		addnormals(models/cinematics/rocket_cin/rocket_local.tga, heightmap(models/cinematics/rocket_cin/rocket_h.tga, 2 ))
	specularmap	models/cinematics/rocket_cin/rocket_s.tga
	
	{ 
		blend add
		map models/cinematics/rocket_cin/rocket_g.tga
		rgb grunttable [ time * 1.3 ]
	}       
        
}
