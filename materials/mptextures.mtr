		colored
	}
}

textures/mptextures/mp_linepulse
{
	qer_editorimage textures/mptextures/mp_linepulse1.tga
	translucent
	noShadows
	nonSolid	
	
	{	//Beam layer (single)
		blend		add
		map 		textures/mptextures/mp_linepulse1.tga
		colored
	}

	{	//Travelling wave

		blend		add
		map		textures/mptextures/mp_linepulse2.tga
		translate	time * Parm4, 0
		colored
	}
}