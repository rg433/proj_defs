textures/mptextures/trim_h3
{
	qer_editorimage textures/mptextures/trim_h3_d

	bumpMap 	textures/mptextures/trim_h3_local
	diffusemap	textures/mptextures/trim_h3_d
	specularmap	textures/mptextures/trim_h3_s

	{
		blend		add
		map			textures/mptextures/trim_h3_g
		red			parm0
		green		parm1
		blue		parm2
	}
}
