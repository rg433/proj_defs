textures/mptextures/trim_h6_blue_pulse
{
	qer_editorimage textures/mptextures/trim_h6_char_pulseref

	bumpMap addnormals ( textures/mptextures/trim_h6_char_local.tga, heightmap ( textures/mptextures/trim_h6_char_h.tga, 1 ) )
	diffusemap	textures/mptextures/trim_h6_char_d
	specularmap	textures/mptextures/trim_h6_char_s
	
	{
		blend add
		map textures/mptextures/trim_h6_char_g_blue
		//rgb 0.75
	}
	{
		blend GL_DST_COLOR, GL_ONE
		map textures/mptextures/trim_h6_char_g_pulse_1
		scroll time*-1, 0
		scale 0.0625, 1
	}
}
