models/monsters/gladiator/shield
{
	unsmoothedTangents	
	
	noShadows
	noSelfShadow
	noDamage
	translucent
	twoSided
	collision
	bounce
	{
		if ( Parm7 <= time )
		blend add
		map models/monsters/gladiator/glad_shieldadd.tga
		vertexColor
		rgb guitable_random1 [time * 2 ]
		scale 1 / 4 , 1
		scroll table4[ time * 6] , 0
	}
	
	{	
		if (Parm7 <= time )
		maskcolor				
		map	makealpha(models/monsters/gladiator/glad_shieldmask.tga)
	}

	{
		if (Parm7 <= time )
		blend GL_DST_ALPHA, gl_one
		maskAlpha
		map models/monsters/gladiator/glad_shieldwave.tga
		rgb .4
		scroll sintable [time * .01 ], sintable [time * .035 ]
		scale  firetable2 [time * .005 ], firetable2 [time * .005 ]
	}
	
	{
		if (Parm7 <= time )
		blend GL_DST_ALPHA, gl_one
		maskAlpha
		map models/monsters/gladiator/glad_shieldwave.tga
		rgb .4
		scroll sintable [time * .015 ], sintable [time * .043 ]
		scale  firetable2 [time * .007 ], firetable2 [time * .007 ]
	}	

	///shield shorting out
	
		{
		if (Parm7 > time )
		blend add
		map models/monsters/gladiator/glad_shieldadd.tga
		vertexColor
//		rgb guitable_random1 [time * 2 ]
		red		fullflicker [time * 2 ]
		green	medFlicker [time * 2 ]
		blue	medFlicker [time * 2]
		scale 1 / 4 , 1
		scroll table4[ time * 6] , 0
	}
	
	{	
		if (Parm7 > time )
		maskcolor				
		map	makealpha(models/monsters/gladiator/glad_shieldmask.tga)
	}

	{
		if (Parm7 > time )
		blend GL_DST_ALPHA, gl_one
		maskAlpha
		map models/monsters/gladiator/glad_shieldwave.tga
		rgb medFlicker [time * .5 ] 
		scroll sintable [time * .01 ], sintable [time * .035 ]
		scale  firetable2 [time * .005 ], firetable2 [time * .005 ]
	}
	
	{
		if (Parm7 > time )
		blend GL_DST_ALPHA, gl_one
		maskAlpha
		map models/monsters/gladiator/glad_shieldwave.tga
		rgb medFlicker [time * 3 ] 
		scroll sintable [time * .015 ], sintable [time * .043 ]
		scale  firetable2 [time * .007 ], firetable2 [time * .007 ]
	}											

}

