		blue	Parm2
	
	}
	
	{
	
		blend	GL_DST_COLOR, GL_SRC_COLOR
		map		gfx/lights/clouds_1.tga
		translate	(time * 0.5 * parm5), (time * -1 * parm5)
		red		Parm0
		green	Parm1
		blue	Parm2
	
	}
	
}


