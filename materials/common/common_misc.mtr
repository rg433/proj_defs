textures/common_misc/pipe1_1_black_to_smolder
{
	qer_EditorImage		textures/common_misc/pipe1_1_black_d.tga
	
	{
		blend		bumpmap
		map		addnormals ( textures/common_misc/pipe1_1_local.tga, heightmap ( textures/common_misc/pipe1_1_black_h.tga, 2 ) )
	}
	
	{
		blend		diffusemap
		map		textures/common_misc/pipe1_1_black_d.tga
		vertexColor
	}
	
	{ 
		blend		specularMap
		map		textures/common_misc/pipe1_1_black_s.tga
		vertexColor
	}
	
	{
		blend 		bumpmap
		map		addnormals ( textures/common_misc/smolder_local.tga, heightmap ( textures/common_misc/smolder_h.tga, 1 ) )
	}
	
	{
		blend 		diffusemap
		map		textures/common_misc/smolder_d.tga
		inverseVertexColor
	}
	
	{
		blend		specularmap
		map		textures/common_misc/smolder_s.tga
		inverseVertexColor
	}
	
	{
		maskcolor
		map		 makealpha ( textures/common_misc/smolder_g_alpha.tga )
		inverseVertexColor
	}
	
	{
		blend		GL_DST_ALPHA, GL_ONE
		maskalpha
		map		textures/common_misc/smolder_g.tga
		rotate		sintable[time * 0.0007]
		rgb		ember1[time * 0.077]
		scale		0.1, 0.1
		scroll		sintable[time * 0.00625], costable[time * 0.0187]
		inverseVertexColor
	}
	
}

