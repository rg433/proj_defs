//Ford's Convoy Retainer Blend---------------------------------------------------------------------------------
textures/common_misc/concrete02_to_impact_middle

{
        //noselfshadow
	materialType rock       
	qer_editorimage textures/common_misc/smolder_d.tga

	{ 
		blend		bumpmap
		map	        textures/common_misc/concrete02_h.tga
	}

	{
		blend		diffusemap
		map		textures/common_misc/concrete02_d.tga
		vertexColor
	}

	{
		blend 		specularmap
		map		textures/common_misc/concrete02_s.tga
		vertexColor
	}
	
	{
		blend 	bumpmap
		map	addnormals ( textures/common_misc/smolder_local.tga, heightmap ( textures/common_misc/smolder_h.tga, 1 ) )
		//inverseVertexColor
	}
	{
		blend 	diffusemap
		map	textures/common_misc/smolder_d.tga
		inverseVertexColor
	}
	{
		blend	specularmap
		map	textures/common_misc/smolder_s.tga
		inverseVertexColor
	}
	{
		maskcolor
		map makealpha(textures/common_misc/smolder_g_alpha.tga)
		inverseVertexColor
		//AlphaZeroClamp
	}
	{
		blend	GL_DST_ALPHA, GL_ONE
		maskalpha
		map	textures/common_misc/smolder_g.tga
		rotate	sintable[time * 0.0007]
		rgb	0.6*ember1[time * 0.057]
		scale	0.1, 0.1
		scroll	sintable[time * 0.00625], costable[time * 0.0187]
		inverseVertexColor
	}

}

