textures/common_misc/fanblade
{
	qer_editorimage textures/common_misc/fanblade_ed.tga

	noshadows

	{       
	 if ( parm6 == 0 )
 
     blend	diffuseMap 
	 map	textures/common_misc/fanblade_d.tga
	 alphaTest .5
	}
//	bumpmap addnormals	( textures/common_misc/fanblade_local.tga, heightmap ( textures/common_misc/fanblade_h.tga, 1 ) )
//	specularmap		textures/common_misc/fanblade_s.tga       
	{       
	 if ( parm6 == 0 )
     blend bumpmap	
	 map	addnormals	( textures/common_misc/fanblade_local.tga, heightmap ( textures/common_misc/fanblade_h.tga, 1 ) )
	 clamp	
	}
	{       
	 if ( parm6 == 0 )
     blend specularmap
	 map	textures/common_misc/fanblade_s.tga
	 clamp	
	}
// rotating stage with parm 

	{       
	 if ( parm6 != 0)
	 blend	diffuseMap 
	 map	textures/common_misc/fanblade_d.tga
	 rotate parm6 * time
	 alphaTest .5
	 clamp	
	}

	{       
	 if ( parm6 != 0 )
     blend bumpmap	
	 map	textures/common_misc/fanblade_local.tga
	 rotate parm6 * time
	 clamp	
	}

	{       

	 if ( parm6 != 0 )
     blend specularmap
	 map	textures/common_misc/fanblade_s.tga
	 rotate parm6 * time
	 clamp	
	}
}

