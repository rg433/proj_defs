textures/common_walls/ctech_wall1_1
{
	qer_EditorImage		textures/common_walls/ctech_wall1_1_d.tga
	materialImage 	textures/common_walls/ctech_wall1_1_hit
	
	bumpMap			addnormals (textures/common_walls/ctech_wall1_1_local.tga, heightmap (textures/common_walls/ctech_wall1_1_h.tga, 1))
	diffuseMap		textures/common_walls/ctech_wall1_1_d.tga
	specularMap		textures/common_walls/ctech_wall1_1_s.tga
	{
		blend		add
		map		textures/common_walls/ctech_wall1_1_add.tga
		red		parm0
		green		parm1
		blue		parm2
	}
}


textures/common_walls/ctech_wall1_1_smolder

{
        noselfshadow
	materialType solidmetal       
	qer_editorimage textures/common_walls/ctech_wall1_1_d.tga

	{ 
		blend	bumpmap
		map	addnormals ( textures/common_walls/ctech_wall1_1_local.tga, heightmap ( textures/common_walls/ctech_wall1_1_h.tga, 1 ) )
		//VertexColor
	}

	{
		blend	diffusemap
		map	textures/common_walls/ctech_wall1_1_d.tga
		VertexColor
	}

	{
		blend 	specularmap
		map	textures/common_walls/ctech_wall1_1_s.tga
		VertexColor
	}

	{
		blend 	bumpmap
		map	addnormals ( textures/common_misc/smolder_local.tga, heightmap ( textures/common_misc/smolder_h.tga, 1 ) )
		//inverseVertexColor
	}
	{
		blend 	diffusemap
		map	textures/common_misc/smolder_d.tga
		inverseVertexColor
	}
	{
		blend	specularmap
		map	textures/common_misc/smolder_s.tga
		inverseVertexColor
	}
	{
		maskcolor
		map makealpha(textures/common_misc/smolder_g_alpha.tga)
		inverseVertexColor
		//AlphaZeroClamp
	}
	{
		blend	GL_DST_ALPHA, GL_ONE
		maskalpha
		map	textures/common_misc/smolder_g.tga
		rotate	sintable[time * 0.0007]
		rgb	ember1[time * 0.077]
		scale	0.1, 0.1
		scroll	sintable[time * 0.00625], costable[time * 0.0187]
		inverseVertexColor
	}

}


