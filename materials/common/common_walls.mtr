textures/common_walls/wall1_4_to_smolder
{
	qer_editorimage		textures/common_misc/smolder_d.tga
	
	{
		blend 	bumpmap
		map	addnormals ( textures/common_misc/smolder_local.tga, heightmap ( textures/common_misc/smolder_h.tga, 1 ) )
		//VertexColor
	}
	
	{
		blend 	diffusemap
		map	textures/common_misc/smolder_d.tga
		inverseVertexColor
	}
	
	{
		blend	specularmap
		map	textures/common_misc/smolder_s.tga
		inverseVertexColor
	}
	
	{
		maskcolor
		map makealpha(textures/common_misc/smolder_g_alpha.tga)
		inverseVertexColor
		//AlphaZeroClamp
	}
	
	{
		blend	GL_DST_ALPHA, GL_ONE
		maskalpha
		map	textures/common_misc/smolder_g.tga
		rotate	sintable[time * 0.0007]
		rgb	ember1[time * 0.077]
		scale	0.1, 0.1
		scroll	sintable[time * 0.00625], costable[time * 0.0187]
		inverseVertexColor
	}
	
	{
		blend bumpmap
		map addnormals ( textures/common_walls/wall1_4_local.tga, heightmap ( textures/common_walls/wall1_4_h.tga, 1 ) )
		vertexcolor
	}
	{
		blend diffusemap
		map textures/common_walls/wall1_4_d.tga
		vertexcolor
	}
	{
		blend specularmap
		map textures/common_walls/wall1_4_s.tga
		vertexcolor
	}
}

