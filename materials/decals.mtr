//	BLASTER MARKS
//----------------------------------

textures/decals/blasterscorch
{
	qer_editorimage textures/decals/blasterscorch.tga
	DECAL_MACRO
	noShadows
	noimpact
	translucent
	{
       	blend GL_DST_COLOR, GL_SRC_COLOR
		map textures/decals/blasterscorch.tga
	}

	{
		blend add
		map textures/decals/blasterscorch_embers.tga
		rgb		guitable_10_5[ time * .4 + VertexRandomizer ]
	}
}

textures/decals/blasterscorch_strip
{
	qer_editorimage textures/decals/blasterscorch_strip.tga
	DECAL_MACRO
	noShadows
	noimpact
	translucent
	{
       	blend GL_DST_COLOR, GL_SRC_COLOR
		map textures/decals/blasterscorch_strip.tga
	}
	
	{
		maskcolor
		map makealpha(textures/decals/blasterscorch_stripa)
	
	}

	{
		blend gl_dst_alpha, gl_one
	    maskalpha
		map textures/decals/blasterscorch_stripg.tga
		//scale 2 * table4[ time * .7 ] , 1 * table4[ time * 1 ]
		//scroll random[ time * .0001 ] , random[ time * .0002 ]
		scroll time * .02, time * .01
		//clamp
	}


	
}

